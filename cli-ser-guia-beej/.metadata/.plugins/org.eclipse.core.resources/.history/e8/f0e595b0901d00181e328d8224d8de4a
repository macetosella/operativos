#include <stdio.h>
#include <stdlib.h>
#include <readline/readline.h>
#include <readline/history.h>

void main() {
  char * linea;
  while(1) {
    linea = readline(">");
    if (!linea) {
      break;
    }
    printf("%s\n", linea);
    free(linea);
  }
}
